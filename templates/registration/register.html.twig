{% extends 'test.html.twig' %}
{%block pagetitle%}
            Register
            {%endblock%}
{% block title %}Register{% endblock %}

{% block body %}
    
    {{ form_start(registrationForm,{'attr': {'novalidate': 'novalidate' }} )}}
        
  
        <fieldset class="mb-3">
            
            <legend>Personal Information</legend>
       
            {{ form_row(registrationForm.photo_de_profile) }}
       
            {{ form_row(registrationForm.nom) }}
            {{ form_row(registrationForm.prenom) }}
       
            {{ form_row(registrationForm.num_telephone) }}
        
            {{ form_row(registrationForm.date_de_naissance) }}
        
    </fieldset>
       <fieldset class="mb-3">
    <legend>Account Information</legend>
    {{ form_row(registrationForm.email) }}
            {{ form_row(registrationForm.username) }}
        
            {{ form_row(registrationForm.plainPassword, {
                label: 'My password :'
            }) }}
        
       </fieldset>
       
        {{ form_row(registrationForm.usertype) }}
       
        
    
    {{ form_widget(registrationForm.Save) }}
    
        
        

        
    {{ form_end(registrationForm) }}
{% endblock %}
