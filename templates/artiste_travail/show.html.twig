{% extends 'artiste.html.twig' %}

{% block title %}Travail{% endblock %}

{% block body %}
    <h1>Travail</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ travail.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ travail.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ travail.prix }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ travail.type }}</td>
            </tr>
            <tr>
                <th>Date_demande</th>
                <td>{{ travail.dateDemande ? travail.dateDemande|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ travail.dateFin ? travail.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ travail.titre }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_artiste_travail_index') }}">back to list</a>

    
{% endblock %}
